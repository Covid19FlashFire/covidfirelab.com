{
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": false,
  "semi": false,
  "useTabs": false,
  "parser": "babel",
  "jsxBracketSameLine": true,
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": [
        "*.json",
        ".prettierrc"
      ],
      "options": {
        "parser": "json-stringify"
      }
    }
  ]
}
